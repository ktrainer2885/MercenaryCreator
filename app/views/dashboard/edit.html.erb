

  <div class="container-fluid">
    <div class="row">

      <main role="main" class="col-sm-9 ml-sm-auto col-md-10 pt-3">
        <h1> Edit <%= @unit.unit_name %>'s Force</h1>

        <div class="row mx-1">
          <form action="/dashboard/update" method ="POST">
            
          <input type="hidden" name="unit_id" value=<%= @unit.id %>>
          <div class="form-group">          
            unit_name: <input type="text" name="unit_name" value="<%= @unit.unit_name %>">
          </div>
          <div class="form-group"> 
            unit_type: <input type="text" name="unit_type" value="<%= @unit.unit_type %>">
          </div>
          <div class="form-group">
            admin_people: <input type="text" name="admin_people" value="<%= @unit.admin_people %>">
          </div>
          <div class="form-group">
              tech_people: <input type="text" name="tech_people" value="<%= @unit.tech_people %>">
          </div>
          <div class="form-group">
              rating: <input type="text" name="rating" value="<%= @unit.rating %>">
          </div>
          <div class="form-group">
              balance: <input type="text" name="balance" value="<%= @unit.balance %>">
          </div>
          <div class="form-group">
              ocost: <input type="text" name="ocost" value="<%= @unit.ocost %>">
          </div>
          
          
          
         
          
        


          <input type="submit" class="btn btn-danger" value="Update">
          </form>
        </div>
        
        </div>
      </main>
    </div>